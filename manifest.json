{
  "name": "Jo The Fish",
  "version": "1.0",
  "description": "An Extension to evaluate sites and businesses on the internet",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs",
    "notifications"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_title":"Jo The Fish",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/happy_jo_icon.png",
      "32": "images/happy_jo_icon.png",
      "48": "images/happy_jo_icon.png",
      "128": "images/happy_jo_icon.png"
    }
  },
  "icons": {
    "16": "images/happy_jo_icon.png",
    "32": "images/happy_jo_icon.png",
    "48": "images/happy_jo_icon.png",
    "128": "images/happy_jo_icon.png"
  },
  "content_security_policy": "script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'",
  "manifest_version": 2
}
